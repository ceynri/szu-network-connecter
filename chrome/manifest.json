{
	"manifest_version": 2,
	"name": "深大校园网自动登录插件",
	"version": "1.0.0",
	"description": "深大校园网自动登录插件",
	"author": "ceynri",
	"icons":
	{
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": 
	{
		"default_icon": "icon.png",
		"default_popup": "index.html"
	},
	"permissions": [
		"notifications",
		"webRequest",
		"webRequestBlocking",
		"storage",
		"http://*/*",
		"https://*/*"
	]
}
